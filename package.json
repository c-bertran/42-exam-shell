{
  "name": "examshell",
  "version": "0.1.0",
  "license": "GPLv3",
  "author": "cbertran",
  "description": "Examshell",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "preinstall": "curl -sf https://gobinaries.com/tj/node-prune | sh",
    "postinstall": "node-prune",
    "build": "pkg --compress Brotli --output dist/examshell .",
    "debug": "pkg --debug --output dist/examshell .",
    "lint": "eslint ."
  },
  "pkg": {
    "assets": [
      "package.json",
      "exam/**/*",
      "srcs/**/*"
    ],
    "targets": [
      "node16-macos-x64",
      "node16-linux-x64"
    ]
  },
  "bin": "index.js",
  "engines": {
    "node": ">=16.13"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "pkg": "^5.5.2"
  },
  "dependencies": {
    "fast-xml-parser": "^4.0.1",
    "inquirer": "^8.2.0"
  }
}
