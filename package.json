{
  "name": "examshell",
  "version": "0.2.1",
  "license": "GPLv3",
  "author": "cbertran",
  "description": "Examshell",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prune": "curl -sf https://gobinaries.com/tj/node-prune | sh; node-prune",
    "build": "pkg --compress Brotli --output dist/examshell .",
    "debug": "pkg --debug --output dist/examshell .",
    "lint": "./node_modules/.bin/eslint ."
  },
  "pkg": {
    "assets": [
      "package.json",
      "exam/**/*",
      "srcs/**/*"
    ],
    "targets": [
      "node16-macos-x64",
      "node16-linux-x64"
    ]
  },
  "bin": "index.js",
  "engines": {
    "node": ">=16.13"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "pkg": "^5.5.2"
  }
}
